@model  IQueryable<ApplicationUser>
@{
    ViewData["Title"] = "Books Users";
}

<h2>Books Application Users</h2>

<table class="table">
    <tr>
        <th>Name</th>
        <th>Id</th>
        <th>Email</th>
    </tr>
    @foreach(var user in Model)
    {
        <tr>
            <td>@user.UserName</td>
            <td>@user.Id</td>
            <td>@user.Email</td>
        </tr>

    }
</table>

